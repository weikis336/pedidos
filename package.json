{
  "dependencies": {
    "connect-redis": "^7.1.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "ioredis": "^5.4.1"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:front-admin dev:auth-admin dev:front-customer dev:auth-customer dev:auth proxy",
